local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({
   Name = "HIDEN HUB",
   LoadingTitle = "Loading Hiden hub UI",
   LoadingSubtitle = "by Simple",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = HIDEN,
      FileName = "Big Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "T7UczcZsRS", 
      RememberJoins = false
   },
   KeySystem = false,
   KeySettings = {
      Title = "HIDEN HUB",
      Subtitle = "Key System",
      Note = "NOKEY",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"NOKEY"}
   }
})

-- TAB
local CREDIT = Window:CreateTab("Credit", 4483362458)
local MENU = Window:CreateTab("MENU", 4483362458)

-- CREDIT TAB
local Label = CREDIT:CreateLabel("Credit : HIDEN HUB")
local Label = CREDIT:CreateLabel("Script Made by : HIDEN HUB")
local Label = CREDIT:CreateLabel("Discord Server : https://discord.gg/hidenhub")

local Button = CREDIT:CreateButton({
   Name = "Copy Discord Server",
   Callback = function()
   local link = "https://discord.gg/hidenhub"
    
    setclipboard(link)
    
    game.StarterGui:SetCore("SendNotification", {Title = "Discord Links";
       Text = "Link copied to clipboard!!";
       Duration = 5;
    })
   end,
})

local Button = CREDIT:CreateButton({
   Name = "Copy Youtube",
   Callback = function()
   local link = "https://youtube.com/@HIDENHUB"
    
    setclipboard(link)
    
    game.StarterGui:SetCore("SendNotification", {Title = "Youtube Links";
       Text = "Link copied to clipboard!!";
       Duration = 5;
    })
   end,
})

local Button = CREDIT:CreateButton({
   Name = "Destroy Ui",
   Callback = function()
  Rayfield:Destroy()
   end,
})

-- Label

local Label = MENU:CreateLabel("Label Example")

-- SECTION
local Section = MENU:CreateSection("Main")


-- BUTTON

local Button = MENU:CreateButton({
   Name = "Button Example",
   Callback = function()
   -- The function that takes place when the button is pressed
   end,
})

-- TOGGLE

local Toggle = MENU:CreateToggle({
   Name = "Toggle Example",
   CurrentValue = false,
   Callback = function(Value)
   -- The function that takes place when the toggle is pressed
   -- The variable (Value) is a boolean on whether the toggle is true or false
   end,
})

-- SLIDER

local Slider = MENU:CreateSlider({
   Name = "Slider Example",
   Range = {0, 100},
   Increment = 10,
   Suffix = "Bananas",
   CurrentValue = 10,
   Callback = function(Value)
   -- The function that takes place when the slider changes
   -- The variable (Value) is a number which correlates to the value the slider is currently at
   end,
})

-- DROPDOWN

local Dropdown = MENU:CreateDropdown({
   Name = "Dropdown Example",
   Options = {"Option 1","Option 2"},
   CurrentOption = {"Option 1"},
   MultipleOptions = false,
   Callback = function(Option)
   -- The function that takes place when the selected option is changed
   -- The variable (Option) is a table of strings for the current selected options
   end,
})
