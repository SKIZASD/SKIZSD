local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({
   Name = "HIDEN HUB",
   LoadingTitle = "Loading Hiden hub UI",
   LoadingSubtitle = "by Simple",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = HIDEN,
      FileName = "Hiden Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "T7UczcZsRS",
      RememberJoins = false
   },
   KeySystem = false,
   KeySettings = {
      Title = "H",
      Subtitle = "Key System",
      Note = "H",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"H"}
   }
})

-- TAB
local CREDIT = Window:CreateTab("Credit", 4483362458)
local Window = Window:CreateTab("MENU", 4483362458)

— CREDIT TAB

local Label = CREDIT:CreateLabel("Credit : Simple")
local Label = CREDIT:CreateLabel("Script Made by : Simple")
local Label = CREDIT:CreateLabel("Discord Server : https://discord.gg/T7UczcZsRS")

local Button = CREDIT:CreateButton({
   Name = "Copy Discord Server",
   Callback = function()
   local link = "https://discord.gg/T7UczcZsRS"
    
    setclipboard(link)
    
    game.StarterGui:SetCore("SendNotification", {Title = "Discord Links";
       Text = "Link copied to clipboard!!";
       Duration = 5;
    })
   end,
})

local Button = CREDIT:CreateButton({
   Name = "Copy Youtube",
   Callback = function()
   local link = "https://youtube.com/@HIDENBUX"
    
    setclipboard(link)
    
    game.StarterGui:SetCore("SendNotification", {Title = "Youtube Links";
       Text = "Link copied to clipboard!!";
       Duration = 5;
    })
   end,
})

local Button = CREDIT:CreateButton({
   Name = "Destroy Ui",
   Callback = function()
  Rayfield:Destroy()
   end,
})

-- Label

local Label = Window:CreateLabel("Label Example")

-- SECTION
local Section = Window:CreateSection("Main")


-- BUTTON

local Button = Window:CreateButton({
   Name = "Button Example",
   Callback = function()
  print(“H”)
   end,
})

-- TOGGLE

local Toggle = Window:CreateToggle({
   Name = "Toggle Example",
   CurrentValue = false,
   Callback = function(Value)
 print(“H”)
   end,
})

-- SLIDER

local Slider = Window:CreateSlider({
   Name = "Slider Example",
   Range = {0, 100},
   Increment = 10,
   Suffix = "Bananas",
   CurrentValue = 10,
   Callback = function(Value)
print(“H”)
   end,
})

-- DROPDOWN

local Dropdown = Window:CreateDropdown({
   Name = "Dropdown Example",
   Options = {"Option 1","Option 2"},
   CurrentOption = {"Option 1"},
   MultipleOptions = false,
   Callback = function(Option)
print(“H”)
   end,
})
